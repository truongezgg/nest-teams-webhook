{
  "name": "@truongezgg/nest-teams-webhook",
  "version": "2.0.1",
  "description": "Send message to Microsoft Teams chanel by webhook",
  "author": "Nguyen Duy Truong <truongezgg@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "keywords": [
    "nestjs",
    "microsft teams webhook",
    "teams webhook",
    "nestjs teams webhook",
    "nestjs microsft teams webhook"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.3",
    "@nestjs/common": "^8.1.1",
    "@nestjs/core": "^8.1.1",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/testing": "8.2.3",
    "@types/jest": "^27.0.2",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "jest": "^27.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "source-map-support": "^0.5.20",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^5.0.0 || ^6.6.0 || ^7.0.0 || ^8.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "homepage": "https://github.com/truongezgg/nest-teams-webhook#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/truongezgg/nest-teams-webhook.git"
  },
  "bugs": {
    "url": "https://github.com/truongezgg/nest-teams-webhook/issues"
  }
}
